pipeline {
  agent any
  stages {
    stage('BUILD') {
      steps {
        echo 'BUILD_STEP_1'
      }
    }
    stage('BUILD_CHANEL') {
      steps {
        sleep(time: 5, unit: 'SECONDS')
      }
    }
    stage('BUILD_FINAL') {
      steps {
        echo 'final'
        zz
      }
    }
    stage('DEPLOY') {
      steps {
        echo 'iyyah'
        
      }
    }
  }
}
